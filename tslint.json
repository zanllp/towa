{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "typedef":false,
        "object-literal-sort-keys":false,
        "ordered-imports":false,
        "max-line-length": [true, 200],
        "arrow-parens": false,
        "quotemark": [true, "single", "jsx-double"],
        "array-type":false,
        "forin":false,
        "no-shadowed-variable":false
    },
    "rulesDirectory": []
}